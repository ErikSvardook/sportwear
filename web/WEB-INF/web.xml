<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <servlet-name>ListaUsuarioServlet</servlet-name>
        <servlet-class>controller.ListaUsuarioServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ElimanarUsuarioServlet</servlet-name>
        <servlet-class>controller.ElimanarUsuarioServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EditarUsuarioServlet</servlet-name>
        <servlet-class>controller.EditarUsuarioServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GuardarEditarUsuarioServlet</servlet-name>
        <servlet-class>controller.GuardarEditarUsuarioServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GuardarCrearUsuarioServlet</servlet-name>
        <servlet-class>controller.GuardarCrearUsuarioServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ListaUsuarioServlet</servlet-name>
        <url-pattern>/ListaUsuarioServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ElimanarUsuarioServlet</servlet-name>
        <url-pattern>/ElimanarUsuarioServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EditarUsuarioServlet</servlet-name>
        <url-pattern>/EditarUsuarioServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GuardarEditarUsuarioServlet</servlet-name>
        <url-pattern>/GuardarEditarUsuarioServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GuardarCrearUsuarioServlet</servlet-name>
        <url-pattern>/GuardarCrearUsuarioServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
